{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "./node_modules/eslint-config-google/index.js",
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}