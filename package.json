{
  "name": "particle_library",
  "version": "0.0.1",
  "description": "A light weight particle library.",
  "main": "main.js",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "test": "mocha test/**/*.test.js -R spec --compilers js:babel-core/register --recursive",
    "build": "webpack --config webpack.prod",
    "dev": "webpack --progress --config webpack.dev && node bin/build",
    "start": "node server.dev.js",
    "serve": "node ./src/back/app.js"
  },
  "keywords": [
    "particles",
    "animation",
    "phyiscs"
  ],
  "author": "Miles Stan Disch",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-register": "^6.16.3",
    "chai": "^3.5.0",
    "eslint": "^3.8.1",
    "eslint-config-google": "^0.7.0",
    "extend": "^3.0.0",
    "glob": "^7.1.1",
    "gutil": "^1.6.4",
    "handlebars": "^4.0.5",
    "html-webpack-plugin": "^2.24.1",
    "mocha": "^3.1.2",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "express": "^4.14.0",
    "extend": "^3.0.0"
  }
}
